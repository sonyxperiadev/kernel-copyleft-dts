&qupv3_se4_i2c {
	status = "ok";
	touchscreen@48 {
		compatible = "sec,sec_ts";
		reg = <0x48>;
		sec,irq_gpio = <&tlmm 25 0x2008>;
		sec,irq_type = <8200>;
		sec,mis_cal_check = <1>;
		sec,regulator_dvdd = "tsp_io";
		sec,regulator_avdd = "tsp_avdd";
		sec,regulator_boot_on;
		sec,i2c-burstmax = <32>;
		sec,project_name = "PJT", "MODEL";
		sec,max_coords = <1644 3840>;   /* x y */
		sec,always_lpmode = <0>;
		sec,bringup = <1>;
		sec,firmware_name = "touch_module_id_0x28.img";
		sec,platform-touch-reset-gpio = <&tlmm 24 0>;
		sec,device_id = /bits/ 8 <0xAC 0x37 0x91>;

		/* feature */
		sec,watchdog_supported = <0>;
		sec,watchdog_delay_ms = <10000>;
		sec,side_touch_supported = <0>;
		sec,glove_mode_supported = <0>;
		sec,cover_mode_supported = <0>;
		sec,aod_mode_supported = <0>;
		sec,sod_mode_supported = <0>;
		sec,stamina_mode_supported = <0>;
	};
};
