&soc {

	somc_pinctrl: somc_pinctrl {
		compatible = "somc-pinctrl";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";
		pinctrl-0 = <&sm_gpio_0 &sm_gpio_1 /*&sm_gpio_2 &sm_gpio_3*/ &sm_gpio_4
			     &sm_gpio_5 &sm_gpio_6 &sm_gpio_7 &sm_gpio_8 &sm_gpio_9
			     &sm_gpio_10 &sm_gpio_12 &sm_gpio_13 &sm_gpio_14 &sm_gpio_15
			     &sm_gpio_16 &sm_gpio_17 &sm_gpio_18 &sm_gpio_19 &sm_gpio_20
			     &sm_gpio_21 &sm_gpio_22 &sm_gpio_23 /*&sm_gpio_24 &sm_gpio_25*/
			     &sm_gpio_40 &sm_gpio_41 &sm_gpio_42 &sm_gpio_43 &sm_gpio_44
			     &sm_gpio_45 &sm_gpio_46 &sm_gpio_47 &sm_gpio_48 &sm_gpio_49
			     &sm_gpio_50 &sm_gpio_51 &sm_gpio_52 &sm_gpio_55 &sm_gpio_56
			     &sm_gpio_57 &sm_gpio_58 &sm_gpio_59 &sm_gpio_60 &sm_gpio_61
			     &sm_gpio_62 &sm_gpio_63 &sm_gpio_64 &sm_gpio_65 &sm_gpio_66
			     &sm_gpio_67 &sm_gpio_68 &sm_gpio_69 &sm_gpio_70 &sm_gpio_71
			     &sm_gpio_72 &sm_gpio_73 &sm_gpio_74 &sm_gpio_75 &sm_gpio_85
			     /*&sm_gpio_86*/ &sm_gpio_87 &sm_gpio_88 &sm_gpio_89 &sm_gpio_90
			     &sm_gpio_91 &sm_gpio_92 &sm_gpio_93 &sm_gpio_97 &sm_gpio_98
			     &sm_gpio_99 &sm_gpio_100 &sm_gpio_101 &sm_gpio_102 &sm_gpio_103
			     &sm_gpio_104 &sm_gpio_105 &sm_gpio_106 &sm_gpio_107 &sm_gpio_109
			     &sm_gpio_110 &sm_gpio_111 &sm_gpio_112 &sm_gpio_113 &sm_gpio_114
			     &sm_gpio_115 &sm_gpio_116 &sm_gpio_117 &sm_gpio_118 &sm_gpio_119
			     &sm_gpio_120 &sm_gpio_121 &sm_gpio_122 &sm_gpio_123 &sm_gpio_124
			     &sm_gpio_125 &sm_gpio_126 &sm_gpio_127 &sm_gpio_128 &sm_gpio_129
			     /*&sm_gpio_133*/ &sm_gpio_137 &sm_gpio_142 &sm_gpio_143 &sm_gpio_145
			     &sm_gpio_151 &sm_gpio_153 &sm_gpio_154 &sm_gpio_155 &sm_gpio_182
			     &sm_gpio_183 &sm_gpio_200 &sm_gpio_206 &sm_gpio_207 &sm_gpio_208
			     &sm_gpio_209>;

		/* If product common default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	somc_pinctrl_pmic: somc_pinctrl_pmic {
		compatible = "somc-pinctrl-pmic";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";

		pinctrl-0 = <&pmk8550_gpio_1 &pmk8550_gpio_2 &pmk8550_gpio_5 &pm8550_gpio_1
			     &pm8550_gpio_2 /*&pm8550_gpio_5*/ /*&pm8550_gpio_6*/ &pm8550_gpio_7
			     &pm8550_gpio_9 /*&pm8550_gpio_10 &pm8550_gpio_11 &pm8550_gpio_12*/
			     &pm8550b_gpio_1 &pm8550b_gpio_2 &pm8550b_gpio_3 /*&pm8550b_gpio_4*/
			     &pm8550b_gpio_5 &pm8550b_gpio_6 /*&pm8550b_gpio_7 &pm8550b_gpio_8*/
			     &pm8550b_gpio_9 &pm8550b_gpio_10 &pm8550b_gpio_12 &pm8550vs_c_gpio_4
			     &pm8550vs_c_gpio_5 &pm8550vs_c_gpio_6 &pm8550vs_d_gpio_4 &pm8550vs_d_gpio_5
			     &pm8550vs_d_gpio_6 &pm8550vs_e_gpio_4 &pm8550vs_e_gpio_5 &pm8550vs_e_gpio_6
			     &pm8550vs_g_gpio_4 /*&pm8550vs_g_gpio_6*/ &pm8550ve_f_gpio_4 &pm8550ve_f_gpio_5
			     &pm8550ve_f_gpio_6 &pm8550ve_f_gpio_7 &pm8550ve_f_gpio_8>;

		/* If product common PMIC default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific PMIC default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	egistec_fp {
		status = "ok";
		compatible = "fp-egistec";
		egistec,gpio_reset = <&tlmm 42 0x00>; 		// reset gpio pin num
		egistec,gpio_irq = <&tlmm 40 0x00>; 		// irq gpio pin num
		egistec,gpio_vcc_en= <&pm8550_gpios 9 0x00>; 	// vcc enable gpio pin num
		fp-ctrl-power;					// enable power control
		fp-gpio-vcc-enable; 				// use gpio enable power or not
		fp-gpio-ext-ldo;                                // yodo use regulator pm_humu_bob1
		pinctrl-names = "egis_rst_high", "egis_rst_low", "egis_vcc_high", "egis_vcc_low", "egis_irq_active", "egis_irq_low";
		pinctrl-0 = <&EGIS_RST_active>;
		pinctrl-1 = <&EGIS_RST_sleep>;
		pinctrl-2 = <&EGIS_VCC_active>;
		pinctrl-3 = <&EGIS_VCC_sleep>;
		pinctrl-4 = <&sm_gpio_40_input_pull_up>;
		pinctrl-5 = <&sm_gpio_40>;
		vcc_fp-supply = <&pm_humu_bob1>;
		egis-fp,vcc-voltage = <3400000 3800000>;
		egis-fp,vcc-current = <1000000>;
		egis-fp,vcc-max-voltage = <3960000>;
	};

	last_logs@ffcc0000 {
		compatible = "rd_last_logs";
		label = "last_logs";
		memory-region = "log";
	};
};

&tlmm {
	EGIS_RST_active: EGIS_RST_active {
		mux {
			pins = "gpio42";
			function = "gpio";
		};

		config {
			pins = "gpio42";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};

	EGIS_RST_sleep: EGIS_RST_sleep {
		mux {
			pins = "gpio42";
			function = "gpio";
		};

		config {
			pins = "gpio42";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};
};

&pm8550_gpios {
	EGIS_VCC_active: EGIS_VCC_active {
		pins = "gpio9";
		function = "normal";
		output-high;
		bias-disable;
		power-source = <1>;
	};

	EGIS_VCC_sleep: EGIS_VCC_sleep {
		pins = "gpio9";
		function = "normal";
		bias-disable;
		output-low;
		power-source = <1>;
	};
};

/* TS_I2C */
&qupv3_se4_i2c {
	pinctrl-0 = <&sm_gpio_44 &sm_gpio_45>;
	pinctrl-1 = <&sm_gpio_44_suspend &sm_gpio_45_suspend>;
	qcom,clk-freq-out = <400000>;
	//qcom,i2c_pull;
	status = "okay";
};

