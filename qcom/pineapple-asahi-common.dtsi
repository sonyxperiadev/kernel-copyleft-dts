&spmi_bus {
	qcom,pmk8550@0 {
		pon_hlos@1300 {
			resin {
				linux,code = <KEY_VOLUMEUP>;
			};
		};
	};
};

&soc {
	gpio_keys {
		pinctrl-0 = <&pm8550_gpio_6
			     &pm8550b_gpio_7
			     &pm8550b_gpio_8>;

		vol_up {
			status = "disabled";
		};

		vol_dn {
			label = "volume_down";
			gpios = <&pm8550_gpios 6 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEDOWN>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};

		cam_snapshot {
			label = "cam_snapshot";
			gpios = <&pm8550b_gpios 7 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <766>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};

		cam_focus {
			label = "cam_focus";
			gpios = <&pm8550b_gpios 8 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_CAMERA_FOCUS>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};
	};
};

&usb0 {
	dwc3@a600000 {
		/* Primary USB maximum-speed */
		maximum-speed = "super-speed";
		/* disable USB2 LPM */
		snps,usb2-lpm-disable;
		snps,usb2-gadget-lpm-disable;
	};
};

&sdhc_2 {
	/* forbid DDR50/SDR104 */
	sdhci-caps-mask = <0x6 0x0>;
	cd-gpios = <&pm8550_gpios 12 GPIO_ACTIVE_HIGH>;
};

