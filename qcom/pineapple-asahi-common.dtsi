			     &sm_gpio_15 &sm_gpio_20 &sm_gpio_21 &sm_gpio_22 &sm_gpio_23
			     &sm_gpio_28 &sm_gpio_46 &sm_gpio_48 &sm_gpio_49 &sm_gpio_50
			     &sm_gpio_51 &sm_gpio_52 &sm_gpio_53 &sm_gpio_54 &sm_gpio_55
			     /*&sm_gpio_56 &sm_gpio_57*/ &sm_gpio_58 &sm_gpio_59 /*&sm_gpio_60*/
			     /* &sm_gpio_61 &sm_gpio_62 &sm_gpio_63*/ &sm_gpio_64 &sm_gpio_65
			     &sm_gpio_66 &sm_gpio_67 &sm_gpio_68 &sm_gpio_69 &sm_gpio_70
			     &sm_gpio_71 &sm_gpio_72 &sm_gpio_73 &sm_gpio_75 &sm_gpio_77
			     &sm_gpio_78 &sm_gpio_79 &sm_gpio_81 &sm_gpio_82 &sm_gpio_83
			     /*&sm_gpio_84*/ &sm_gpio_85 /*&sm_gpio_86*/ &sm_gpio_87 &sm_gpio_88
			     &sm_gpio_89 &sm_gpio_90 &sm_gpio_91 &sm_gpio_92 &sm_gpio_93
			     &sm_gpio_97 &sm_gpio_98 &sm_gpio_99 &sm_gpio_100 &sm_gpio_101
			     &sm_gpio_102 &sm_gpio_103 &sm_gpio_104 &sm_gpio_105 &sm_gpio_106
			     &sm_gpio_108 &sm_gpio_109 &sm_gpio_110 &sm_gpio_111 &sm_gpio_112
			     &sm_gpio_113 &sm_gpio_114 &sm_gpio_115 &sm_gpio_116 &sm_gpio_117
			     &sm_gpio_118 &sm_gpio_119 &sm_gpio_120 /*&sm_gpio_121 &sm_gpio_122*/
			     /*&sm_gpio_123*/ &sm_gpio_124 &sm_gpio_125 &sm_gpio_126 &sm_gpio_127
			     &sm_gpio_128 &sm_gpio_129 /*&sm_gpio_133*/ &sm_gpio_137 &sm_gpio_144
			     &sm_gpio_151 &sm_gpio_153 &sm_gpio_155 /*&sm_gpio_161 &sm_gpio_162*/
			     &sm_gpio_163 &sm_gpio_164 &sm_gpio_182 &sm_gpio_183 &sm_gpio_188
			     &sm_gpio_189 &sm_gpio_200 &sm_gpio_201 &sm_gpio_206 &sm_gpio_207
			     &sm_gpio_208 &sm_gpio_209>;

		/* If product common default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	somc_pinctrl_pmic: somc_pinctrl_pmic {
		compatible = "somc-pinctrl-pmic";
		pinctrl-names = "platform_common_default",
				"product_common_default",
				"variant_default";

		pinctrl-0 = <&pmk8550_gpio_1 &pmk8550_gpio_2 &pmk8550_gpio_5 &pm8550_gpio_1
			     &pm8550_gpio_2 &pm8550_gpio_3 /*&pm8550_gpio_6*/ &pm8550_gpio_9
			     &pm8550_gpio_11 /*&pm8550_gpio_12*/ &pm8550b_gpio_1 &pm8550b_gpio_2
			     &pm8550b_gpio_3 &pm8550b_gpio_4 &pm8550b_gpio_5 &pm8550b_gpio_6
			     /*&pm8550b_gpio_7 &pm8550b_gpio_8*/ &pm8550b_gpio_9 &pm8550b_gpio_10
			     &pm8550b_gpio_12 &pm8550vs_c_gpio_4 &pm8550vs_c_gpio_5 &pm8550vs_c_gpio_6
			     &pm8550vs_d_gpio_4 &pm8550vs_e_gpio_4 &pm8550vs_e_gpio_5 &pm8550vs_e_gpio_6
			     /*&pm8550vs_g_gpio_4*/ &pm8550vs_g_gpio_6 &pm8550ve_gpio_4 &pm8550ve_gpio_5
			     &pm8550ve_gpio_6 &pm8550ve_gpio_7 &pm8550ve_gpio_8>;

		/* If product common PMIC default setting is needed,
		fill pinctrl-1 value in <product>_common.dtsi */
		pinctrl-1 = <>;

		/* If variant specific PMIC default setting is needed,
		fill pinctrl-2 value in <variant>.dtsi */
		pinctrl-2 = <>;
	};

	last_logs@ffcc0000 {
		compatible = "rd_last_logs";
		label = "last_logs";
		memory-region = "log";
	};
	egistec_fp {
		status = "ok";
		compatible = "fp-egistec";
		egistec,gpio_reset = <&tlmm 89 0x00>;			// reset gpio pin num
		egistec,gpio_irq = <&tlmm 92 0x00>;			// irq gpio pin num
		egistec,gpio_vcc_en= <&pm8550_gpios 9 0x00>;		// vcc enable gpio pin num
		fp-ctrl-power;						// enable power control
		fp-gpio-vcc-enable;					// use gpio enable power or not
		fp-gpio-ext-ldo;					// yodo use regulator pm_humu_bob1
		pinctrl-names = "egis_rst_high", "egis_rst_low", "egis_vcc_high", "egis_vcc_low", "egis_irq_active", "egis_irq_low";
		pinctrl-0 = <&EGIS_RST_active>;
		pinctrl-1 = <&EGIS_RST_sleep>;
		pinctrl-2 = <&EGIS_VCC_active>;
		pinctrl-3 = <&EGIS_VCC_sleep>;
		pinctrl-4 = <&sm_gpio_92_input_pull_up>;
		pinctrl-5 = <&sm_gpio_92>;
		vcc_fp-supply = <&pm_humu_bob1>;
		egis-fp,vcc-voltage = <3400000 3800000>;
		egis-fp,vcc-current = <1000000>;
		egis-fp,vcc-max-voltage = <3960000>;
	};
};

&tlmm {
	EGIS_RST_active: EGIS_RST_active {
		mux {
			pins = "gpio89";
			function = "gpio";
		};

		config {
			pins = "gpio89";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};

	EGIS_RST_sleep: EGIS_RST_sleep {
		mux {
			pins = "gpio89";
			function = "gpio";
		};

		config {
			pins = "gpio89";
			drive-strength = <2>;
			bias-disable;
			output-low;
		};
	};
};

&pm8550_gpios {
	EGIS_VCC_active: EGIS_VCC_active {
		pins = "gpio9";
		function = "normal";
		output-high;
		drive-push-pull; /* Digital Out */
		qcom,drive-strength = <PMIC_GPIO_STRENGTH_LOW>;
		power-source = <1>;
	};

	EGIS_VCC_sleep: EGIS_VCC_sleep {
		pins = "gpio9";
		function = "normal";
		output-low;
		drive-push-pull; /* Digital Out */
		qcom,drive-strength = <PMIC_GPIO_STRENGTH_LOW>;
		power-source = <1>;
	};
};

