/* arch/arm64/boot/dts/qcom/pineapple-asahi-pdx245_common.dtsi */

#include "pineapple-asahi-common.dtsi"

&soc {
};
&somc_pinctrl {
	/* If product common default setting is needed,
	fill pinctrl-1 value in <product>_common.dtsi */
	pinctrl-1 = <&sm_gpio_100 &sm_gpio_126 &sm_gpio_127 &sm_gpio_129>;
};

/* GPIO_100 : CAM_MCLK0 */
&sm_gpio_100 {
	mux {
		pins = "gpio100";
		function = "cam_mclk";
	};

	config {
		pins = "gpio100";
		drive-strength = <2>;
		/delete-property/ bias-pull-down;
		bias-disable;
		/delete-property/ input-enable;
	};
};

/* GPIO_126 : NC */
&sm_gpio_126 {
	mux {
		pins = "gpio126";
		function = "gpio";
	};

	config {
		pins = "gpio126";
		drive-strength = <2>;
		/delete-property/ bias-disable;
		bias-pull-down;
		input-enable;
	};
};

