/* arch/arm64/boot/dts/qcom/waipio-nagara-cdb.dtsi */

#include "waipio-nagara-common.dtsi"

&somc_pinctrl {
	/* If variant specific default setting is needed,
	fill pinctrl-2 value in <variant>.dtsi */
	pinctrl-2 = </*&sm_gpio_4 &sm_gpio_5*/ &sm_gpio_8 &sm_gpio_22
		     &sm_gpio_54 &sm_gpio_55 &sm_gpio_59 &sm_gpio_60
		     &sm_gpio_61 &sm_gpio_62 &sm_gpio_63 &sm_gpio_67
		     &sm_gpio_84 &sm_gpio_104 &sm_gpio_105 &sm_gpio_108
		     &sm_gpio_118 &sm_gpio_121 &sm_gpio_122 &sm_gpio_123
		     &sm_gpio_124 &sm_gpio_205>;

};

&somc_pinctrl_pmic {
	/* If variant specific default setting is needed,
	fill pinctrl-2 value in <variant>.dtsi */
	pinctrl-2 = <&pm8350_gpio_8 &pm8350b_gpio_1 &pm8350b_gpio_3 &pm8350b_gpio_4 &pm8350b_gpio_6>;
};

&soc {
};

/* GPIO_8: VIB_LDO_EN */
&pm8350_gpio_8 {
	pins = "gpio8";
	function = "normal";
	/delete-property/ bias-high-impedance;
	output-low;
	drive-push-pull;
	qcom,drive-strength = <PMIC_GPIO_STRENGTH_LOW>;
	power-source = <1>;
};

