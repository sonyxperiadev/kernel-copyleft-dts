/* GPIO_4 : WLC_I2C_SDA */
&sm_gpio_4 {
	mux {
		pins = "gpio4";
		function = "qup1";
	};

	config {
		pins = "gpio4";
		drive-strength = <2>;
		/delete-property/ bias-pull-down;
		bias-disable;
		/delete-property/ input-enable;
	};
};


