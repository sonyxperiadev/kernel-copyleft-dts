		gpios = <&tlmm 100 0>, <&tlmm 116 0>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK0", "SONY_CAMERA_RESET";
		cci-device = <0>;
