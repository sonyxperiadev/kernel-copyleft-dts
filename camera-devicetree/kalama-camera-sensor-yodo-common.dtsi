		cam_vdig-supply = <&L1N>;
		cam_vdig2-supply = <&L2N>;
		cam_vana-supply = <&slg51000_0_ldo2>;
		cam_vana2-supply = <&camera1_vana_vreg>;
		cam_vaf-supply = <&L3N>;
		cam_vaf2-supply = <&L4N>;
		cam_ois-supply = <&L6N>;
		cam_ois2-supply = <&L7N>;
		cam_clk-supply = <&cam_cc_titan_top_gdsc>;
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&sm_gpio_100 &sm_gpio_116>;
