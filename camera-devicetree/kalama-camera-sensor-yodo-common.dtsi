		clocks = <&camcc CAM_CC_MCLK0_CLK>;
		clock-names = "cam_clk";
		clock-rates = <24000000>;
		use_spi = <0>;
		match_id = <0x40 0x0000 0x02 0x0650>;
		thremal_enable = <1>;
		thermal_names = "sony_cam_back1", "sony_cam_back1_ext";

		LGI12BC0 {
			i2c_freq_mode = <3>;
			power_off {
				commands =
					"gpio_reset",
					"cam_clk",
					"cam_ois2",
					"cam_ois",
					"cam_vaf2",
					"cam_vaf",
					"cam_vdig",
					"cam_vdig2",
					"cam_vana",
					"cam_vana2",
					"cam_vio",
					"cam_pmic0",
					"exit";
				gpio_reset = <7 0x0 0 5>;
				cam_clk = <5 0xffffffff 0 0>;
				cam_ois2 = <12 0xffffffff 0 1000>;
				cam_ois = <11 0xffffffff 0 0>;
				cam_vaf2 = <4 0xffffffff 0 1000>;
				cam_vaf = <3 0xffffffff 0 0>;
				cam_vdig2 = <20 0xffffffff 0 0>;
				cam_vdig = <0 0xffffffff 0 0>;
				cam_vana2 = <30 0xffffffff 0 5000>;
				cam_vana = <2 0xffffffff 0 0>;
				cam_vio = <1 0xffffffff 0 1000>;
				cam_pmic0 = <40 0xffffffff 0 0>;
				exit = <99 0x0 0 0>;
			};
			power_on {
				commands =
					"cam_pmic0",
					"cam_vio",
					"cam_vana2",
					"cam_vana",
					"cam_vdig2",
					"cam_vdig",
					"cam_vaf",
					"cam_vaf2",
					"cam_ois",
					"cam_ois2",
					"cam_clk",
					"gpio_reset",
					"exit";
				cam_pmic0 = <40 0 0 10000>;
				cam_vio = <1 1860 0 1>;
				cam_vana2 = <30 0 0 5000>;
				cam_vana = <2 2800 0 1>;
				cam_vdig2 = <20 900 0 1>;
				cam_vdig = <0 810 0 1>;
				cam_vaf = <3 2800 0 0>;
				cam_vaf2 = <4 2800 0 0>;
				cam_ois = <11 2800 0 0>;
				cam_ois2 = <12 2800 0 0>;
				cam_clk = <5 24000000 0 1>;
				gpio_reset = <7 1 0 1100>;
				exit = <99 0x0 0 0>;
			};
		};
	};

